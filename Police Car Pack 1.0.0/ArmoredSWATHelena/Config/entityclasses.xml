<configs>
	<append xpath="/entity_classes">
		<entity_class name="vehicleArmoredSWAT">
			<property name="Tags" value="vehicle"/>
			<property name="Class" value="EntityVJeep"/>
			<property name="Parent" value="Cars"/>
			<property name="Prefab" value="#@modfolder:Resources/AutoBlindado.unity3d?Blindado"/>
			<property name="ModelType" value="Standard"/>
			<property name="SurfaceCategory" value="metal"/>
			<property name="IgnoreTrigger" value="true"/>
			<property name="LootListAlive" value="ArmoredSWATStorage"/>
			<property name="IsEnemyEntity" value="false"/>
			<property name="Explosion.ParticleIndex" value="6"/> 
			<property name="Explosion.RadiusBlocks" value="24"/>
			<property name="Explosion.RadiusEntities" value="5"/>
			<property name="Explosion.BlockDamage" value="10000"/>
			<property name="Explosion.EntityDamage" value="10000"/>
			<property name="RotateToGround" value="true"/>
			<property name="Faction" value="none"/>
			<property name="MapIcon" value="SwatMap"/>
			<property name="NavObject" value="SwatMap" />
		</entity_class>

		<entity_class name="vehicleArmoredSWATCustom">
			<property name="Tags" value="vehicle"/>
			<property name="Class" value="EntityVJeep"/>
			<property name="Parent" value="Cars"/>
			<property name="Prefab" value="#@modfolder:Resources/AutoBlindadoCustom.unity3d?Blindado"/>
			<property name="ModelType" value="Standard"/>
			<property name="SurfaceCategory" value="metal"/>
			<property name="IgnoreTrigger" value="true"/>
			<property name="LootListAlive" value="ArmoredSWATStorage"/>
			<property name="IsEnemyEntity" value="false"/>
			<property name="Explosion.ParticleIndex" value="6"/> 
			<property name="Explosion.RadiusBlocks" value="24"/>
			<property name="Explosion.RadiusEntities" value="5"/>
			<property name="Explosion.BlockDamage" value="10000"/>
			<property name="Explosion.EntityDamage" value="10000"/>
			<property name="RotateToGround" value="true"/>
			<property name="Faction" value="none"/>
			<property name="MapIcon" value="SwatMap"/>
			<property name="NavObject" value="SwatMap" />
		</entity_class>

		
	</append>
</configs>